{
  "root": true,
  "env": {
    "browser": true
  },
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "react-hooks",
    "@typescript-eslint"
  ],
  "extends": [
    "airbnb"
  ],
  "rules":  {
    "react/jsx-filename-extension": [2, {
      "extensions": [".js", ".jsx", ".ts", ".tsx"]
    }],
    "import/no-unresolved": 0,
    "import/extensions": 0
  }
}